    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "~5.2",
    "css-loader": "^3.0.0",
    "cssnano": "~4.1",
    "del-cli": "~1.1",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "file-loader": "^4.0.0",
    "html-loader": "^0.5.5",
