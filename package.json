{
  "name": "Altair",
  "version": "1.0.0",
  "description": "WEB-UI for ProjectSummerTriangle/Vega",
  "homepage": "https://github.com/ProjectSummerTriangle/Altair",
  "license": "MIT",
  "scripts": {
    "build:aot:prod": "npm run clean:dist && npm run clean:aot && cross-env BUILD_AOT=1 NODE_ENV=production webpack --config webpack.config.js --progress --profile --bail",
    "build:aot": "npm run build:aot:prod",
    "build:prod": "cross-env NODE_ENV=production webpack --config webpack.config.js  --progress --profile --bail",
    "build": "npm run build:prod",
    "clean:aot": "npm run rimraf -- compiled",
    "clean:dist": "npm run rimraf -- dist",
    "clean:install": "npm set progress=false && npm install",
    "clean:start": "npm start",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist",
    "prebuild:dev": "npm run clean:dist",
    "prebuild:prod": "npm run clean:dist",
    "preclean:install": "npm run clean",
    "preclean:start": "npm run clean",
    "rimraf": "rimraf",
    "server:dev:hmr": "npm run server:dev -- --inline --hot",
    "server:dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.js --progress --profile --watch --content-base src/",
    "server:aot:dev": "cross-env BUILD_AOT=1 npm run server:dev",
    "start:hmr": "npm run server:dev:hmr",
    "start": "npm run server:dev",
    "test": "karma start",
    "tslint": "tslint",
    "version": "npm run build",
    "watch:dev:hmr": "npm run watch:dev -- --hot",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:prod": "npm run build:prod -- --watch",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "watch": "npm run watch:dev",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "npm run webdriver-manager update",
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack",
    "postinstall": "node scripts/font-fix.js"
  },
  "dependencies": {
    "@angular/animations": "~8.0.0",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/compiler-cli": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/forms": "~8.0.0",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/platform-server": "~8.0.0",
    "@angular/router": "~8.0.0",
    "core-js": "~2.6",
    "deneb-ui": "^1.0.0",
    "downloadjs": "^1.4.7",
    "hammerjs": "^2.0.8",
    "ie-shim": "0.1.0",
    "intersection-observer": "^0.5.0",
    "moment": "^2.19.3",
    "ng2-date-picker": "^2.6.2",
    "ngx-swiper-wrapper": "^8.0.2",
    "popper.js": "^1.12.9",
    "reflect-metadata": "^0.1.9",
    "rxjs": "^6.5.2",
    "semantic-ui-less": "^2.2.4",
    "url-polyfill": "^1.1.5",
    "web-animations-js": "^2.2.5",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-optimizer": "^0.801.2",
    "@ngtools/webpack": "~8.0.0",
    "@types/chrome": "^0.0.58",
    "@types/downloadjs": "^1.4.0",
    "@types/hammerjs": "^2.0.33",
    "@types/node": "~10.12",
    "@types/source-map": "^0.5.0",
    "@types/uglify-js": "^2.0.27",
    "angular-router-loader": "~0.8",
    "angular2-template-loader": "~0.6",
    "chalk": "^1.1.3",
    "clean-webpack-plugin": "~0.1",
    "codelyzer": "^5.1.0",
    "compression-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "cssnano": "~4.1",
    "del-cli": "~1.1",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "file-loader": "^4.0.0",
    "fork-ts-checker-webpack-plugin": "^1.3.7",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "~3.2",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.7.0",
    "optimize-css-assets-webpack-plugin": "~5.0",
    "parse5": "^3.0.1",
    "raw-loader": "^3.0.0",
    "rimraf": "^2.5.2",
    "semantic-ui-less-module-loader": "^1.0.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "~0.23",
    "terser-webpack-plugin": "^1.3.0",
    "to-string-loader": "^1.1.5",
    "ts-loader": "^6.0.2",
    "tslib": "^1.10.0",
    "tslint": "^5.19.0",
    "typescript": "~3.4",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "url-loader": "^0.5.7",
    "webpack": "~4.29",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "~3.2",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-dev-server": "^3.7.1",
    "webpack-merge": "~4.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ProjectSummerTriangle/Altair.git"
  },
  "bugs": {
    "url": "https://github.com/ProjectSummerTriangle/Altair/issues"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  },
  "sideEffects": [
    "./src/polyfills.browser.ts",
    "./src/main.browser.ts",
    "./src/assets/semantic-ui.ts",
    "./node_modules/semantic-ui-less/semantic.less",
    "./src/app/app.less",
    "*.css"
  ]
}
