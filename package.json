    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.7.0",
    "optimize-css-assets-webpack-plugin": "~5.0",
    "parse5": "^3.0.1",
    "raw-loader": "^3.0.0",
    "rimraf": "^2.5.2",
    "semantic-ui-less-module-loader": "^1.0.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "~0.23",
    "to-string-loader": "^1.1.5",
    "ts-loader": "^6.0.2",
    "tslib": "^1.10.0",
